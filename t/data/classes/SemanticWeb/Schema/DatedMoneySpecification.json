            "schema:DatedMoneySpecification",
            "schema:InvestmentOrDeposit",
            "schema:LoanOrCredit"
         ],
         "schema:rangeIncludes" : [
            "schema:Number",
            "schema:MonetaryAmount"
         ]
      },
      "schema:currency" : {
